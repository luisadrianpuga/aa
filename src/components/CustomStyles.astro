---
import '@fontsource/inter/variable.css';

// Nunito
// Dosis
---

<style is:inline is:global>
  :root {
    --aw-font-sans: 'InterVariable';
    --aw-font-serif: var(--aw-font-sans);
    --aw-font-heading: var(--aw-font-sans);

    --aw-color-primary: #BFA979; /* gold */
    --aw-color-secondary: #000000; /* black */
    --aw-color-accent: #BFA979; /* gold */
    --aw-color-text-page: #000000; /* black */
    --aw-color-text-muted: #999999; /* gray */
    --aw-color-bg-page: #ffffff; /* white */
  }
</style>
